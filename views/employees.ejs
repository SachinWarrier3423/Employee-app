<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Employee List</title>
</head>
<body>
  <h2>All Employees</h2>
  <table border="1">
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Email</th>
      <th>Experience</th>
      <th>Actions</th>
    </tr>
    <% employees.forEach(emp => { %>
    <tr>
      <td><%= emp.name %></td>
      <td><%= emp.age %></td>
      <td><%= emp.email %></td>
      <td><%= emp.experience %></td>
      <td>
        <!-- Example actions -->
        <a href="/employees/<%= emp._id %>/edit">Edit</a> |
        <a href="/employees/<%= emp._id %>/delete">Delete</a>
      </td>
    </tr>
    <% }) %>
  </table>
  <br>
  <a href="/employees/new">Add New Employee</a> |
  <a href="/logout">Logout</a>
</body>
</html>
